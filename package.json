{
  "name": "application-airbag-detection",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup-py": "IF NOT EXIST .venv (py -m venv .venv) & .venv\\Scripts\\python -m pip install -U pip & .venv\\Scripts\\python -m pip install -r requirements.txt",
    "fastapi-dev": ".venv\\\\Scripts\\\\python -m uvicorn api.main:app --reload --host 0.0.0.0 --port 8000 --log-level info",
    "next-dev": "next dev --port 3000 --hostname 0.0.0.0",
    "dev": "concurrently -k -n \"next,api\" -c \"cyan,magenta\" \"npm:next-dev\" \"npm:fastapi-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint"
  },
  "dependencies": {
    "lucide": "^0.541.0",
    "lucide-react": "^0.541.0",
    "next": "15.5.0",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "tailwindcss": "^4.1.12",
    "typescript": "^5"
  }
}
